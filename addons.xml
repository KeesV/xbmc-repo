<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon	id="repository.keesv"
		name="KeesV's repo"
		version="1.0.0"
		provider-name="KeesV">
	<extension	point="xbmc.addon.repository"
				name="KeesV's repo">
		<info compressed="false">https://raw.github.com/KeesV/xbmc-repo/master/addons.xml</info>
		<checksum>https://raw.github.com/KeesV/xbmc-repo/master/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.github.com/KeesV/xbmc-repo/master/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Install KeesV's repo</summary>
		<description>Download and install KeesV's repo</description>
		<disclaimer></disclaimer>
		<platform>all</platform>
	</extension>
</addon>

<addon id="script.service.movielights"
	version="0.0.2"
	name="XBMC Movie Lights"
	provider-name="KeesV">
	<requires>
	</requires>
	<extension point="xbmc.service" library="addon.py">
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Controls lights based on XBMC status</summary>
		<description>Controls lights based on XBMC status</description>
		<platform>all</platform>
	</extension>

</addon>
</addons>
